<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- 
        OWASP Dependency Check Suppressions for ClusterFuzz Java
        
        This file contains suppressions for known false positives or accepted risks
        in the ClusterFuzz Java implementation.
    -->
    
    <!-- Spring Framework - Known false positives -->
    <suppress>
        <notes><![CDATA[
            Spring Framework CVEs that don't apply to our usage pattern
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework/.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- Jackson - Known false positives for specific versions -->
    <suppress>
        <notes><![CDATA[
            Jackson CVEs that are mitigated by our configuration
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/.*$</packageUrl>
        <cve>CVE-2018-1000873</cve>
    </suppress>
    
    <!-- Logback - Configuration-dependent vulnerabilities -->
    <suppress>
        <notes><![CDATA[
            Logback vulnerabilities that don't apply to our secure configuration
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/ch\.qos\.logback/.*$</packageUrl>
        <cve>CVE-2017-5929</cve>
    </suppress>
    
    <!-- Test dependencies - Not in production -->
    <suppress>
        <notes><![CDATA[
            Test-only dependencies that don't affect production security
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\..*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
            Test-only dependencies that don't affect production security
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <!-- Development tools - Not in production -->
    <suppress>
        <notes><![CDATA[
            Development and build tools that don't affect runtime security
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.maven\.plugins/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <!-- Google Cloud dependencies - Managed by Google -->
    <suppress>
        <notes><![CDATA[
            Google Cloud SDK dependencies are managed and updated by Google
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.google\.cloud/.*$</packageUrl>
        <vulnerabilityName regex="true">.*google.*</vulnerabilityName>
    </suppress>
    
    <!-- Specific known false positives -->
    <suppress>
        <notes><![CDATA[
            False positive: This CVE doesn't apply to our usage of the library
        ]]></notes>
        <sha1>ABCDEF1234567890ABCDEF1234567890ABCDEF12</sha1>
        <cve>CVE-YYYY-NNNN</cve>
    </suppress>
    
</suppressions>